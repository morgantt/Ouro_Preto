<table class="profile" summary="Profile information">
  <tr>
    <td class="main">
      <h1>
      <%= @corretor.nome %>
      </h1>
      <h5>
      <%= @corretor.banco %>
      </h5>
    </td>
    <td class="sidebar round">
   
     <%= link_to "Editar Dados Corretor", edit_corretor_path(@corretor)%><br />		   			
     <%= link_to "Adicionar Limites",  new_cparametro_path(@corretor)%><br />
     <%= link_to "Associar Propostas", associa_path(@corretor)%><br />
    </td>
  </tr>
</table>
	
	

<%= search_form_for @q do |f| %>
  <%= f.label :numero %>
  <%= f.text_field :numero_cont %>

  <%= f.submit %>
<% end %>	
<table class="profile" summary="Profile information">
	<tr>
		
		<th WIDTH="12%">Numero </th>
		<th WIDTH="5%"> Operadora </th>
		<th WIDTH="15%"> Tipo </th>
		<th WIDTH="15%"> Status </th>
		<th WIDTH="13%"> Data </th>
		<th WIDTH="40%"> Corretor </th>
		

	</tr>
</table>

<%= render @propostashow %>
<%= will_paginate @propostashow %>

